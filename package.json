{
  "name": "pomodoro_go",
  "version": "2.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "main": "index.js",
  "scripts": {
    "build": "INLINE_RUNTIME_CHUNK=false webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "NODE_ENV=development npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route",
    "prepare": "husky install"
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "apexcharts": "^3.26.3",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "faker": "^5.5.3",
    "generate-password": "^1.6.0",
    "google-auth-library": "^7.1.0",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
    "query-string": "^7.0.0",
    "react-apexcharts": "^1.3.9",
    "react-bootstrap": "^1.6.0",
    "request": "^2.88.2",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "esm": "^3.2.25",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^6.2.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^4.2.0",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "supertest": "^4.0.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.3.1"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{js,jsx,css}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2101-Warriors/pomodoro-go.git"
